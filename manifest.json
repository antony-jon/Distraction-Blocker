{
    "manifest_version": 3,
    "name": "AI Web Blocker",
    "version": "1.0",
    "description": "Blocks unsafe websites using AI analysis.",
    "permissions": ["storage", "scripting", "webNavigation", "tabs", "alarms"],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}
